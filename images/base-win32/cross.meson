[constants]
arch = 'i686-w64-mingw32'

[binaries]
c = arch + '-gcc'
cpp = arch + '-g++'
ar = arch + '-gcc-ar'
ranlib = arch + '-gcc-ranlib'
strip = arch + '-strip'
windres = arch + '-windres'
dlltool = arch + '-dlltool'
nasm = 'nasm'
exe_wrapper = ['wine']

[properties]
needs_exe_wrapper = true

[host_machine]
system = 'windows'
cpu_family = 'x86'
cpu = 'i686'
endian = 'little'
