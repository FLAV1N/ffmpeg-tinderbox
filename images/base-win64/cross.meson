[constants]
arch = 'x86_64-w64-mingw32'

[binaries]
c = arch + '-gcc'
cpp = arch + '-g++'
ar = arch + '-gcc-ar'
ranlib = arch + '-gcc-ranlib'
strip = arch + '-strip'
windres = arch + '-windres'
dlltool = arch + '-dlltool'
exe_wrapper = ['wine']

[properties]
needs_exe_wrapper = true

[host_machine]
system = 'windows'
cpu_family = 'x86_64'
cpu = 'x86_64'
endian = 'little'
